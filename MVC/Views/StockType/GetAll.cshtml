@using Entities.Concrete
@model List<Entities.Concrete.StockType>

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

@{
    Layout = null;
}
<table class="table table-hover">
    <tr>
        <th>Id</th>
        <th>Stok Türü</th>
        <th>Durum</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Deleted</td>
        </tr>
    }
</table>
