@{
    ViewData["Title"] = "Home Page";
}

@using Entities.Concrete
@model List<Entities.Concrete.StockType>


<table class="table table-hover">
    <tr>
        <th>Id</th>
        <th>Stok Türü</th>
        <th>Durum</th>
        <th>İşlemler</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Deleted</td>
        </tr>
    }
</table>
<a href="/StockType/Add" class="btn btn-info">Yeni Stock Tipi</a>
